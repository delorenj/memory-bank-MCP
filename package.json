{
  "name": "@delorenj/memory-bank-mcp",
  "version": "2.1.0",
  "description": "Implements a file based memory bank and provides a set of tools for interacting with it.",
  "main": "dist/index.js",
  "type": "module",
  "private": false,
  "bin": {
    "memory-bank-mcp": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
