    "start": "node dist/index.js",
    "dev": "NODE_OPTIONS=\"--loader ts-node/esm --experimental-specifier-resolution=node\" ts-node src/index.ts"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.0",
    "@modelcontextprotocol/sdk": "^1.8.0",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "mcp",
    "memory-bank",
    "gemini",
    "ai"
  ],
  "author": "Jarad DeLorenzo",
  "license": "MIT"
}
